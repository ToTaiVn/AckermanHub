if getgenv().Language == "English" then
loadstring(game:HttpGet("https://raw.githubusercontent.com/ToTaiVn/AckermanXSimple/main/toshunranksss.lua"))()
elseif getgenv().Language == "Vietnamese" then
loadstring(game:HttpGet("https://raw.githubusercontent.com/ToTaiVn/AckermanXSimple/main/toshunvietnamlaso1.lua"))()
else
game.Players.LocalPlayer:Kick("Bro Wrong Language!!!")
end
